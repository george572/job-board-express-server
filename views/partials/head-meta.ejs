    var u="https://samushaoge.matomo.cloud/";
    _paq.push(['setTrackerUrl', u+'matomo.php']);
    _paq.push(['setSiteId', '1']);
    var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
    g.async=true; g.src='https://cdn.matomo.cloud/samushaoge.matomo.cloud/matomo.js'; s.parentNode.insertBefore(g,s);
  })();
</script>
